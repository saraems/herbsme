{
  "compilerOptions": {
    "target": "ES2019",
    "lib": [
      "ES2020",
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "outDir": "./build/incremental/kahoot-frontend",
    "incremental": true,
    "jsx": "react",
    "baseUrl": ".",
    "noUnusedLocals": true,
    "noEmitOnError": false,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "isolatedModules": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
